<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="NewPersistenceUnit" transaction-type="JTA">
        <jta-data-source>Soundworld</jta-data-source>

        <class>musique.Musique</class>
        <class>utilisateur.Utilisateur</class>
        <properties>
            <property name="toplink.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:JEE"/>
            <property name="toplink.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
        </properties>

    </persistence-unit>
</persistence>
